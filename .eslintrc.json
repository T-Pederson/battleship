{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["*test.js", "*config.js", "*Mock.js"],
    "rules": {
        "class-methods-use-this": "off",
        "consistent-return": "off",
        "no-empty": "off",
        "brace-style":"off",
        "no-restricted-syntax": "off",
        "no-loop-func": "off",
        "no-param-reassign": "off"
    }
}
