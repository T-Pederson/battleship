{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["*test.js"],
    "rules": {
        "max-classes-per-file": "off",
        "class-methods-use-this": "off",
        "consistent-return": "off",
        "no-empty": "off",
        "brace-style":"off"
    }
}
